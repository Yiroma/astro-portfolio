---
const {
  class: className = "h-6 w-6",
  viewBox = "0 0 24 24",
  width = "24",
  height = "24",
  fill = "currentColor",
} = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox={viewBox}
  width={width}
  height={height}
  fill={fill}
  class:list={` ${className}`}
>
  <slot />
</svg>
